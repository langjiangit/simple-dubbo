<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:soa="http://www.tstd2.com/schema/soa"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
						http://www.springframework.org/schema/beans/spring-beans.xsd
						http://www.tstd2.com/schema/soa
						http://www.tstd2.com/shcema/soa.xsd">

    <soa:registry protocol="redis" address="127.0.0.1:6380" />
    <!--<soa:registry protocol="zookeeper" address="10.10.40.27:2181" />-->

    <soa:protocol port="9000" threads="200" serialize="hession2" />

    <bean id="calcService" class="com.tstd2.soa.xml.service.CalcServiceImpl" />
    <soa:service interface="com.tstd2.soa.xml.service.CalcService" ref="calcService" timeout="1000" />

    <bean id="userService" class="com.tstd2.soa.xml.service.UserServiceImpl" />
    <soa:service interface="com.tstd2.soa.xml.service.UserService" ref="userService" timeout="1000" />

</beans>